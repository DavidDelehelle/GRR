<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta name="Robots" content="noindex" />
		<title>GRR - {{ settings.company }}</title>
		<link rel="shortcut icon" href="../favicon.ico" />
		<link rel="stylesheet" type="text/css" href="../bootstrap/css/bootstrap.min.css?v={{d.versionCache}}" />
		<link rel="stylesheet" type="text/css" href="../bootstrap/css/mod_bootstrap.css?v={{d.versionCache}}" />
		<link rel="stylesheet" type="text/css" href="../bootstrap/css/jquery-ui.css?v={{d.versionCache}}" />
		<link rel="stylesheet" type="text/css" href="../node_modules/bootstrap/dist/css/jquery-ui-timepicker-addon.css?v={{d.versionCache}}" >
		<link rel="stylesheet" type="text/css" href="../themes/default/css/style.css?v={{d.versionCache}}" />
		<link rel="stylesheet" type="text/css" href="../{{ d.sheetcss }}?v={{d.versionCache}}" />
		{{ d.sheetcssperso|raw }}
		<link rel="stylesheet" type="text/css" href="./node_modules/@fortawesome/fontawesome-free/css/all.min.css" />
		{% block css %}{% endblock %}
	</head>

	<body>
		<header>
			<div id="panel">
				<div class="logo" height="100">
					{% if d.logo != ''%}<a href="{{ d.pageAccueil |raw }}"><img src="{{ d.logo }}" alt="logo"></a>{% endif %}
				</div>
				<div class="accueil">
					<h2><a href="{{ d.pageAccueil |raw }}">{{ settings.company }}</a></h2>
					<p>{{ settings.message_accueil |raw }}</p>
				</div>
				<div class="administration">
					{% if d.lienAdmin != '' %}<br><a href='../{{ d.lienAdmin |raw }}'>{{ trad.admin }}</a>{% endif %}
					{% if d.nbConnecte %}<br><a href="../admin/admin.php?p=admin_view_connexions">{{ d.nbConnecte }}</a>{% endif %}
					{% if d.mess_resa %}<br><p class='avertissement'><a href='?p=moderationsliste' class='avertissement'>{{ d.mess_resa }}</a></p>{% endif %}
				</div>
				<div class="configuration" >
					{% if d.jsHeure %}
					<div class="clock">
						<div id="Date">
							&nbsp;<span id="hours"></span>h<span id="min"></span>
						</div>
					</div>
					{% endif %}
					<a onclick="charger();" href="{{ d.urlLangue }}?{{ d.paramUrl |raw }}&amp;default_language=fr"><img src="../img_grr/fr_dp.png" alt="France" title="france" width="20" height="13" class="image" /></a>
					<a onclick="charger();" href="{{ d.urlLangue }}?{{ d.paramUrl |raw }}&amp;default_language=de"><img src="../img_grr/de_dp.png" alt="Deutch" title="deutch" width="20" height="13" class="image" /></a>
					<a onclick="charger();" href="{{ d.urlLangue }}?{{ d.paramUrl |raw }}&amp;default_language=en"><img src="../img_grr/en_dp.png" alt="English" title="English" width="20" height="13" class="image" /></a>
					<a onclick="charger();" href="{{ d.urlLangue }}?{{ d.paramUrl |raw }}&amp;default_language=it"><img src="../img_grr/it_dp.png" alt="Italiano" title="Italiano" width="20" height="13" class="image" /></a>
					<a onclick="charger();" href="{{ d.urlLangue }}?{{ d.paramUrl |raw }}&amp;default_language=es"><img src="../img_grr/es_dp.png" alt="Spanish" title="Spanish" width="20" height="13" class="image" /></a>
					<br /><a href="./compte.php?{{ d.paramUrl |raw }}">{{ d.nomUtilisateur }} - {{ trad.manage_my_account }}</a>
					{% if d.accesStats %}<br /><a href="../app.php?p=report">{{ trad.report }}</a>{% endif %}
					{{ d.lienDeconnexion |raw }}
					{{ d.lienConnexion |raw }}
				</div>
			</div>
			<a id="open" class="open" href="#"><i class="fa-solid fa-arrows-up-down"></i></a>
		</header>
		
		<section>
			{% if d.masquerMenu != 1 %}
				<div id="menu-compte" class="col col-lg-2 col-md-2 col-xs-12">
					<br><br><a href="compte.php?pc=moncompte" class="btn btn-primary col-lg-12">Mon compte</a>
					<br><br><a href="compte.php?pc=mesconnexions" class="btn btn-primary col-lg-12">Mes connexions</a>
					<br><br><a href="compte.php?pc=mesreservations" class="btn btn-primary col-lg-12">Mes r√©servations</a>
				</div>
			{% endif %}

			<div id="page-compte" class="col col-lg-10 col-md-10 col-xs-12">
				{% block content %}{% endblock %}
			</div>

			<div id="popup_name" class="popup_block"></div>
		</section>
		

		<script type="text/javascript" src="../node_modules/jquery/dist/jquery.min.js?v={{d.versionCache}}"></script>
		<!--<script type="text/javascript" src="../node_modules/jquery/dist/jquery-ui.min.js?v={{d.versionCache}}"></script>
		<script type="text/javascript" src="../js/jquery.validate.js?v={{d.versionCache}}"></script>-->
		<script type="text/javascript" src="../js/jquery-ui-timepicker-addon.js?v={{d.versionCache}}"></script>
		<script type="text/javascript" src="../node_modules/bootstrap/js/bootstrap.min.js?v={{d.versionCache}}"></script>
		<script type="text/javascript" src="../js/popup.js?v={{d.versionCache}}" charset="utf-8"></script>
		<script type="text/javascript" src="../js/functions.min.js?v={{d.versionCache}}" ></script>
		<script type="text/javascript" src="../js/selection.js?v={{d.versionCache}}" ></script>
		{% if d.jsHeure %}
			<script type="text/javascript" src="../js/{{ d.jsHeure }}?v={{d.versionCache}}"></script>
		{% endif %}
		{% block javascript %}{% endblock %}

	</body>
</html>